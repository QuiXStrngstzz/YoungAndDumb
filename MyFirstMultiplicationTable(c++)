#include <iostream>

using namespace std;

int main ()
{
	int num, prod;
	
	cout << "Enter A Number: ";
	cin  >> num;
	
	cout << "The Multiplacation table of " << num << " is:\n";
	
	for (int i = 1; i <= 10; i++)
	{
    prod = num * i;
		cout << num << " x " << i << " = " << prod << endl;
	}	
	return 0;
}
